<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.6"?>
<plugin>
 <extension
       id="org.eclipse.cdt.environmentVars.Nature"
       name="Environment vars nature"
       point="org.eclipse.core.resources.natures">
    <runtime>
       <run
             class="io.sloeber.autobuild.cdt.ui.ProjectNature1">
       </run>
    </runtime>
 </extension>

 <extension
         point="org.eclipse.ui.propertyPages">
        <page
            class="io.sloeber.autobuild.cdt.ui.Page_head_build"
            id="io.sloeber.autoBuild.projectProperties.main"
            name="%page.c.build"
            >
         <enabledWhen>
           <adapt type="org.eclipse.core.resources.IResource">
	            <test property="org.eclipse.core.resources.projectNature"
	                  value="org.eclipse.cdt.environmentVars.Nature"/>
           </adapt>
         </enabledWhen>
        </page>    
        <page
            class="org.eclipse.cdt.managedbuilder.ui.properties.Page_Variables"
            id="io.sloeber.autoBuild.projectProperties.macro"
            category="io.sloeber.autoBuild.projectProperties.main"
            name="%CDTVariablesProperty.name"
            >
         <enabledWhen>
           <adapt type="org.eclipse.core.resources.IProject">
	            <test property="org.eclipse.core.resources.projectNature"
	                  value="org.eclipse.cdt.environmentVars.Nature"/>
           </adapt>
         </enabledWhen>
      </page>
        <page
              category="io.sloeber.autoBuild.projectProperties.main"
              class="org.eclipse.cdt.managedbuilder.ui.properties.Page_Environment"
              id="io.sloeber.autoBuild.projectProperties.EnvironmentVars"
              name="%Environment">
           <enabledWhen>
              <adapt
                    type="org.eclipse.core.resources.IProject">
                 <test
                       property="org.eclipse.core.resources.projectNature"
                       value="org.eclipse.cdt.environmentVars.Nature">
                 </test>
              </adapt>
           </enabledWhen>
        </page>
