 <extension
       point="org.eclipse.cdt.ui.cPropertyTab">
    <tab
          class="io.sloeber.autobuild.cdt.ui.BuilderSettingsTab"
          helpId="cdt_u_prop_build"
          icon="icons/obj16/build_settings.gif"
          name="%Builder.settings"
          parent="io.sloeber.autobuild.cdt.ui.Page_head_build"
          weight="040">
    </tab>
    <tab
          class="io.sloeber.autobuild.cdt.ui.BuildBehaviourTab"
